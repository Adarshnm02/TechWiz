<%- include('../partials/admin/adminHeader') %>
  <style>
    .tableData {
      padding-top: 75px !important;
    }

    .tableDataBtn {
      padding-top: 46px !important;
    }
  </style>

  <div class="col-lg-10 grid-margin stretch-card container-fluid ">
    <div class="card">
        <div class="card-body border-bottom">
                <div class="border-bottom address-div">

                            <div class="d-flex justify-content-between align-items-center mb-3">
                                <img src="../../user/img/user-1.jpg" alt="Profile Image" width="100"
                                    height="100">
                                <h4></h4>
                                <div class="edit-delete-buttons">
                                        <p class="text-danger">Canceled</p>
                                </div>
                            </div>
                            <table class="table">
                                <tbody class="border-bottom">
                                    <!-- Display limited rows -->
                                    <tr data-toggle="collapse" data-target="#expandableRows"
                                        aria-expanded="false" aria-controls="expandableRows">
                                        <td>Product</td>
                                        <td>:</td>
                                        <td></td>
                                    </tr>
                                    <tr data-toggle="collapse" data-target="#expandableRows"
                                        aria-expanded="false" aria-controls="expandableRows">
                                        <td>Quantity</td>
                                        <td>:</td>
                                        <td></td>
                                    </tr>
                                    <tr data-toggle="collapse" data-target="#expandableRows"
                                        aria-expanded="false" aria-controls="expandableRows">
                                        <td>Price</td>
                                        <td>:</td>
                                        <td></td>
                                    </tr>
                                    <tr data-toggle="collapse" data-target="#expandableRows"
                                        aria-expanded="false" aria-controls="expandableRows">
                                        <td>Email</td>
                                        <td>:</td>
                                        <td></td>
                                    </tr>
                                    <!-- Collapsible rows -->
                                    <tr id="expandableRows" class="collapse">
                                        <td>Status</td>
                                        <td>:</td>
                                        <td></td>
                                    </tr>
                                    <tr id="expandableRows" class="collapse">
                                        <td>Address</td>
                                        <td>:</td>
                                        <td></td>
                                    </tr>
                                </tbody>
                            </table>

                </div>
        </div>
    </div>
  </div>

  <%- include('../partials/admin/adminFooter') %>